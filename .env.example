# Environment Configuration Template
# Copia este archivo como .env y ajusta los valores

# Environment
ENV=development

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_TIMEOUT=30

# Model Configuration
MODEL_VERSION=v2_finetuned
MODEL_PATH=models/cnn_modelo_v2_finetuned/model.onnx

# Logging
LOG_LEVEL=INFO
LOG_DIR=logs

# Performance
ENABLE_CUDA=true
MAX_BATCH_SIZE=64
NUM_WORKERS=4

# Streamlit
STREAMLIT_API_URL=http://127.0.0.1:8000
